l=[]
s=str(input())
n=len(s)
l.append(s)
for i in range(n-1):
    x=str(input())
    l.append(x)

sve=''
y=''
for k in l:
    for j in k:
        sve=sve+j
lista=[]
for c in range(len(s)):
    for r in range(c,len(sve),n):
        y=y+sve[r]
    lista.append(y)
    y=''
for m in lista:
    for b in m:
        print(b.upper(), end=' ')
    print('')
if l==lista:
    print('True')
else:
    print('False')
